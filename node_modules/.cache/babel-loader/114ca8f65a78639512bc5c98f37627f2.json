f (elementOutofView(el)) {\n      hideScrollElement(el)\n    }\n  })\n}\nwindow.addEventListener(\"scroll\", () => { \n  handleScrollAnimation();\n});\n  }}\n \n}\n\n</script>\n\n<style lang=\"scss\">\n  @import './src/assets/scss/main.scss';\n  .site{\n    padding: 0 16px;\n  }\n\n  header, .site-content{\n    margin-bottom: 128px;\n  }\n\n  .js-scroll{\n    opacity: 0;\n    transition: opacity 500ms ;\n  }\n\n  .js-scroll.scrolled{\n    opacity: 1;\n  }\n</style>\n"],"sourceRoot":""}]}